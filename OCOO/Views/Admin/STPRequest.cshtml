@model OCOO.Models.Dashboard
@{
    ViewData["Title"] = "STP Request";
    Layout = "~/Views/Shared/_Layout.cshtml";
}



<div class="page-wrapper">
    <div class="row page-titles">
        <div class="col-md-6 align-self-center">
            <h3 class="text-themecolor">Dashboard</h3>
            
        </div>
    </div>
    <div class="container-fluid">
                <div class="clearfix">
                    <div class="card-body p-3 pb-20">
                        <div class="row">
                            @for (int i = 1; i < Model.dtDetails.Rows.Count; i++)
                            {
                                <div class="col-md-6 ">
                                    <a href="/Admin/GetFirm?Id=@Crypto.Encrypt(@Model.dtDetails.Rows[i]["Id"].ToString())">
                                        <div class="card">
                                            <div class="card-body p-4 text-center font-weight-bold">
                                                @Model.dtDetails.Rows[i]["Name"].ToString()
                                            </div>
                                        </div>
                                    </a>
                                </div>
                            }
                        </div>
                    </div>
                </div>
            </div>
       
</div>
