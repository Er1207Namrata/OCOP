            $('#TotalLeg').text(TotalLeg);
            $('#PcountLeg1').text(PcountLeg1);
            $('#PcountLeg2').text(PcountLeg2);
            $('#TotalBusiness').text(TotalBusiness);
            document.getElementById("mySidenav").style.width = "314px";
            $(".sidenav").css({ 'display': 'block' });

        }

